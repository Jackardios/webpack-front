{
  "name": "webpack-front",
  "version": "1.2.0",
  "description": "Frontend boilerplate and framework based on webpack, pug and sass",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist && rm -rf build && rm -rf public_html",
    "clean--win": "rd /s /q dist && rd /s /q build && rd /s /q public_html",
    "build": "webpack --env=development",
    "build:production": "npm run clean && webpack -p --env=production",
    "start": "webpack-dev-server --env=development"
  },
  "keywords": [
    "sass",
    "scss",
    "webpack",
    "optimized",
    "boilerplate",
    "html",
    "css",
    "js"
  ],
  "author": "Jackardios <salahutdinov.salavat@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/register": "^7.4.0",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^2.0.1",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "import-glob": "^1.5.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-normalize": "^7.0.1",
    "postcss-preset-env": "^6.6.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.2.3",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.0",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.8.1",
    "bootstrap": "^4.3.1"
  }
}
