{
  "name": "webpack-front",
  "version": "1.0.0",
  "description": "Frontend boilerplate and framework based on webpack, pug and sass",
  "main": "index.js",
  "scripts": {
    "clean": "rm dist -r -f && rm build -r -f",
    "clean--win": "rd /s /q dist && rd /s /q build",
    "prod": "npm run clean--win && webpack -p --env=production",
    "build": "npm run clean--win && webpack --env=production",
    "dev": "webpack-dev-server --env=development"
  },
  "keywords": [
    "pug",
    "sass",
    "scss",
    "webpack",
    "boilerplate",
    "html",
    "css",
    "js"
  ],
  "author": "Jackardios <salahutdinov.salavat@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "css-loader": "^0.28.4",
    "css-mqpacker": "^6.0.1",
    "cssnano": "^3.10.0",
    "external-svg-sprite-loader": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.29.0",
    "image-webpack-loader": "^3.3.1",
    "import-glob": "^1.5.0",
    "node-sass": "^4.5.3",
    "normalize-scss": "^7.0.0",
    "postcss-loader": "^2.0.6",
    "pug": "^2.0.0-rc.2",
    "pug-loader": "^2.3.0",
    "resolve-url-loader": "^2.1.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "svgo": "^0.7.2",
    "svgo-loader": "^1.2.1",
    "url-loader": "^0.5.9",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.6.1",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "jquery": "^3.2.1",
    "webpack-jquery-ui": "^1.0.0",
    "svgxuse": "^1.2.4"
  }
}
